@article{hinds_government_1997,
  title = {Government {{Policy}} and the {{Nigerian Palm Oil Export Industry}}, 1939-49},
  volume = {38},
  issn = {0021-8537},
  abstract = {Throughout the period 1939 to 1949 the lack of expansion in palm oil exports from Nigeria was a source of much concern to imperial and colonial government officials. To the former, it limited Nigeria's contribution both to the alleviation of metropolitan and worldwide shortages in fats and oils, and more significantly, Britain's efforts to reconstruct its post-war economy. To the latter, it jeopardized the future of the heavily populated eastern region of Nigeria and the long-term welfare of the industry. However, despite their concerns both the imperial and colonial governments were unable to devise a coherent set of policies to secure an expansion in export production. In retrospect failure was inevitable, because for most of the period under review, colonial government policy was opposed to a radical overhaul in the system of oil palm production and changes were predicated on the notion that the industry should remain a peasant industry. Consequently, between 1942 and 1948 Nigerian oil palm producers did not capitalize on the opportunity for expansion which resulted from the international shortage in fats and oils. By 1948, the failure to expand export production threatened to destabilize the economy of the eastern region, the centre of the Nigerian oil palm produce industry. Confronted by this reality, the colonial authorities accepted that the oil palm produce industry could not survive without the introduction of a plantation system.},
  number = {3},
  url = {http://www.jstor.org/stable/182544},
